{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "production build",
			"type": "shell",
			"command": "npm",
			"args": ["install"],
			"problemMatcher": ["$tsc"]
		},
		{
			"label": "debug build",
			"type": "shell",
			"command": "npm",
			"args": ["run", "build:debug"],
			"problemMatcher": ["$tsc"],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
